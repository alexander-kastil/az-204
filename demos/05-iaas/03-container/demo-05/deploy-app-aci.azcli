env=32294
grp=az204-m05-containers
loc=westeurope
acr=configacr$env
fqdn=azurecr.io
apiapp=config-api
uiapp=config-ui
apiimg=arambazamba/$apiapp
ngimg=$acr.$fqdn/$uiapp

az acr login --name $acr
az acr update -n $acr --admin-enabled true
pwd=$(az acr credential show -n $acr --query passwords[0].value -o tsv)

# net api
url=$(az container create -g $grp -l $loc -n configapi --image arambazamba/config-api  --cpu 1 --memory 1 --dns-name-label configapi --port 80 --query ipAddress.fqdn -o tsv)
echo $url

env=dev
loc=westeurope
grp=foodapp-$env
acr=az204demos$env
img=food-shopping-cart

az group create -n $grp -l $loc
az acr create -g $grp -n $acr --sku Basic
az acr login --name $acr
az acr update -n $acr --admin-enabled true

cd $img
az acr build --image $img --registry $acr --file Dockerfile .
cd ..
